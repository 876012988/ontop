[PrefixDeclaration]
:		http://www.semanticweb.org/elem/ontologies/2015/11/mesowest#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[SourceDeclaration]
sourceUri	mesowest
connectionUrl	jdbc:postgresql://localhost/mesowest
username	postgres
password	
driverClass	org.postgresql.Driver

[MappingDeclaration] @collection [[
mappingId	rain
target		:Rain/{station_id}/{currTime}/{nextTime} a :Rain ; :startTime {currTime} ; :endTime {nextTime} . 
source		SELECT station_id, currTime, currP1, nextTime, nextP1
			FROM (SELECT tb_newyorkdata2012."date_time" AS currTime, precip_accum_one_hour_set_1 AS currP1,
			lead(tb_newyorkdata2012."date_time", 1) OVER () AS nextTime,
			lead(tb_newyorkdata2012."precip_accum_one_hour_set_1") OVER () AS nextP1,
			station_id
			FROM tb_newyorkdata2012
			WHERE station_id = 'KFZY') AS a
			WHERE nextP1 > currP1
]]

