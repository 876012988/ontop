{
	"db" : {
		"url" : "some/url",
		"username" : "user",
		"password" : "password",
		"driver" : "mongo.driver"
	},
    "prefixes": "xsd:",
    "mappings": [
        {
            "mappingId": "UndergraduateStudent",
            "source": {
                "collection": "students",
                "criteria": {
                    "type": 1
                }
            },
            "target": "<http://ex.org/Student/{code}> a <http://ex.org/UndergraduateStudent> ."
        }
        ,

        {
            "mappingId": "GraduateStudent",
            "source": {
                "collection": "students",
                "criteria": {
                    "type": 2
                }
            },
            "target": "<http://ex.org/Student/{code}> a <http://ex.org/GraduateStudent> ."
        }
        ,

        {
            "mappingId": "Student",
            "source": {
                "collection": "students",
                "criteria": {}
            },
            "target": "<http://ex.org/Student/{code}> a :Student ; ex:name {name}^^xsd:string ; ex:email {contact.email}^^xsd:string ; <http://ex.org/age> {age}^^xsd:int ; <http://ex.org/nickname> {nicknames[$]} ."
        }
    ]
}