PREFIX mt:	   <http://www.semanticweb.org/ontologies/2018/4/mimic/temporal/>
PREFIX ms:     <http://www.semanticweb.org/ontologies/2018/4/mimic/>
PREFIX obda:   <https://w3id.org/obda/vocabulary#>
PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
BASE <http://www.semanticweb.org/ontologies/2018/4/mimic/temporal/>

?p rdf:type mt:PatientAffectedByLongLastingFever :- {
    ALWAYS IN PAST [0, 7D] {
        ?p mt:hasBodyTemp ?v . (?v > 38.5)
    }
}

?p rdf:type mt:PatientRecentlyAffectedByLongLastingFeverInICU :- {
    SOMETIME IN PAST [0, 14D] {
        ?p rdf:type mt:ICUStay . ?p rdf:type mt:PatientAffectedByLongLastingFever .
    }
}

?p rdf:type mt:AdultICUPatientExcludedFromHIVClinicalTrial :- {
    ?p rdf:type ms:Adult . ?p rdf:type mt:PatientRecentlyAffectedByLongLastingFeverInICU .
}

?p rdf:type mt:ICUStayFirstDay :- {
    SOMETIME IN PAST [0, 24H] {
        ?p rdf:type mt:AdmittedToICU .
    }
    ?p rdf:type mt:ICUStay .
}

?p mt:hasFirstDayCreatinineLevel ?v :- {
    ?p mt:hasCreatinineLevel ?v .
    ?p rdf:type mt:ICUStayFirstDay .
}

?p rdf:type mt:ICUPatientDressedArterialLineCatheter :- {
    ?p rdf:type mt:ICUStay .
    ALWAYS IN PAST [0, 4D] {
        ?p rdf:type mt:ArterialLineCatheterDressed .
    }
}