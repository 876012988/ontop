[PrefixDeclaration]
:		http://siemens.com/temporal/ns#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	urn:Map-rotorSpeed
target		:RotationSpeedSensor/{sensor_id} :rotorSpeed {value}.
interval    [{ts_begin}^^xsd:dateTimeStamp, {ts_end}^^xsd:dateTimeStamp)
source		SELECT tb_measurement.sensor_id, value, timestamp AS ts_begin, LEAD(timestamp, 1) OVER (PARTITION BY tb_measurement.sensor_id ORDER BY timestamp) AS ts_end FROM tb_measurement, tb_sensors WHERE tb_measurement.sensor_id = tb_sensors.sensor_id AND sensor_type = 0

mappingId	urn:Map-mainFlameOn
target		:MainFlameOn/{sensor_id} a :MainFlameOn.
interval    [{ts_begin}^^xsd:dateTimeStamp, {ts_end}^^xsd:dateTimeStamp)
source		SELECT sensor_id, value, ts_begin, ts_end FROM (SELECT tb_measurement.sensor_id AS sensor_id, value, timestamp AS ts_begin, LEAD(timestamp, 1) OVER (PARTITION BY tb_measurement.sensor_id ORDER BY timestamp) AS ts_end FROM tb_measurement, tb_sensors WHERE tb_measurement.sensor_id = tb_sensors.sensor_id AND sensor_type = 1) SUBQ WHERE value > 1
]]