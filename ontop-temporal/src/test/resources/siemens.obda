[PrefixDeclaration]
:		http://siemens.com/ns#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	urn:Map-TempSensor
target		:TemperatureSensor/{sensor_id} a :TemperatureSensor ;  :measures {m_type}^^xsd:string .
source		SELECT sensor_id, 'Temperature' AS m_type FROM tb_sensors WHERE sensor_type =1

mappingId	urn:Map-Burner
target		:Burner/{turbine_id}/{component_id} a :Burner . 
source		SELECT component_id, turbine_id FROM tb_components WHERE component_type =1

mappingId	urn:Map-isMonitoredBy
target		:TurbinePart/{mnted_part} :isMonitoredBy :Sensor/{sensor_id} . 
source		SELECT mnted_part, sensor_id FROM tb_sensors

mappingId	urn:Map-RotSpeedSensor
target		:RotationSpeedSensor/{sensor_id} a :RotationSpeedSensor ;  :measures {sensor_id}^^xsd:integer .
source		SELECT sensor_id FROM tb_sensors WHERE sensor_type = 0

mappingId	urn:Map-PowerTurbine
target		:PowerTurbine/{turbine_id}/{component_id} a :PowerTurbine . 
source		SELECT turbine_id, component_id FROM tb_components WHERE component_type = 0
]]

