[PrefixDeclaration]
:		http://siemens.com/ns#
st:		http://siemens.com/temporal/ns#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	urn:Map-rotorSpeed
target		:RotationSpeedSensor/{sensor_id} st:rotorSpeed {value}.
interval    {rs_begin_inc} {rs_begin}^^xsd:dateTimeStamp, {rs_end}^^xsd:dateTimeStamp {rs_end_inc}
source		SELECT "tb_measurement"."sensor_id", "value", CAST('TRUE' AS Boolean) AS "rs_begin_inc", "timestamp" AS "rs_begin", LEAD("timestamp", 1) OVER (PARTITION BY "tb_measurement"."sensor_id" ORDER BY "timestamp") AS "rs_end", CAST('FALSE' AS Boolean) AS "rs_end_inc" FROM "public"."tb_measurement", "public"."tb_sensors" WHERE "tb_measurement"."sensor_id" = "tb_sensors"."sensor_id" AND "tb_sensors"."sensor_type" = 0

mappingId	urn:Map-mainFlame
target		:TemperatureSensor/{sensor_id} st:mainflame {value}.
interval    {ts_begin_inc} {ts_begin}^^xsd:dateTimeStamp, {ts_end}^^xsd:dateTimeStamp {ts_end_inc}
source		SELECT "tb_measurement"."sensor_id", "value", CAST('TRUE' AS Boolean) AS "ts_begin_inc", "timestamp" AS "ts_begin", LEAD("timestamp", 1) OVER (PARTITION BY "tb_measurement"."sensor_id" ORDER BY "timestamp") AS "ts_end", CAST('FALSE' AS Boolean) AS "ts_end_inc" FROM "public"."tb_measurement", "public"."tb_sensors" WHERE "tb_measurement"."sensor_id" = "tb_sensors"."sensor_id" AND "tb_sensors"."sensor_type" = 1
]]